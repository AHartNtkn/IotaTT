module Empty where

cEmpty : * = (A : *) -> A

Empty : * = i (e : cEmpty) . (P : cEmpty -> *) -> P e

EmptyInd : (P : Empty -> *) -> (e : Empty) -> P e
         = \ P e .
               e.2
                 (\ (x : cEmpty) . (X : *) -> ((x' : Empty) -> (x ~ x') -> P x' -> X) -> X)
                 (P e)
                 (\x' eq u . r(x . P x) (r(o . o ~ e) eq . B) . u)

EmptyInd2 : (P : Empty -> *) -> (e : Empty) -> P e = \ P e . e.1 (P e)